    <style name="AppBaseTheme" parent="android:Theme.Light"/>
    <style name="AppTheme" parent="AppBaseTheme"/>
    <style name="WePayTheme" parent="AppBaseTheme">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="wepay_common_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <!-- 浮动在屏幕中间 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- dialog背景 -->
        <item name="android:background">@android:color/transparent</item>
        <!-- dialog内容背景 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 显示window背景 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 窗口半透明 -->
    </style>
    <style name="wepay_content_page_large_count_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff8295bf</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="wepay_content_page_large_text">
        <item name="android:duplicateParentState">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">@color/wepay_text_main_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="wepay_content_page_small_text">
        <item name="android:duplicateParentState">true</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/wepay_txt_unenable</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="wepay_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="wepay_divider_line">
        <item name="android:layout_width">@dimen/wepay_divider_line_height</item>
        <item name="android:layout_height">@dimen/wepay_divider_line_height</item>
        <item name="android:background">@color/wepay_divider_common</item>
    </style>
    <style name="wepay_edit_divider_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/wepay_divider_line_height</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:background">#FFFFFF</item>
    </style>
    <style name="wepay_edit_normal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:layout_height">@dimen/wepay_widget_height</item>
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColorHint">@color/wepay_txt_hint</item>
    </style>
    <style name="wepay_txt_normal">
        <item name="android:textSize">@dimen/wepay_txt_middle</item>
        <item name="android:textColor">@color/wepay_txt_title</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/wepay_txt_unenable</item>
    </style>
</resources>